---
---
[{% assign clients = site.projects | where: 'client', true %}{% for client in clients reversed %}{
    "id": {{ client.id | jsonify }},
    "name": {{ client.title | jsonify }},
    "url": {{ client.web_site | jsonify }},
    "role": {{ client.role | jsonify }},
    "from": {{ client.start_date | jsonify }}{% unless client.end_date == 'present' %},
    "to": {{ client.end_date | jsonify }}{% endunless %}
}{% unless forloop.last %},{% endunless %}{% endfor %}{% assign clients = nil %}]