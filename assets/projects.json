---
---
[{% for project in site.projects reversed %}{
    "id": {{ project.id | jsonify }},
    "name": {{ project.title | jsonify }},
    "role": {{ project.role | jsonify }},
    "from": {{ project.start_date | jsonify }},{% if project.end_date %}{% unless project.end_date == 'present' %}
    "to": {{ project.end_date | jsonify }},{% endunless %}{% endif %}
    "summary": {{ project.summary | jsonify }},
    "skills": {{ project.skills | jsonify }},
    "excerpt": {{ project.excerpt | jsonify }},
    "content": {{ project.content | jsonify }}
}{% unless forloop.last %},{% endunless %}{% endfor %}{% assign project = nil %}]